{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier/react"
  ],
  "plugins": [
    "jest",
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/destructuring-assignment": ["warn", "never"],
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "prettier/prettier": "error"
  }
}
